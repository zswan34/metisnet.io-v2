{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "bacon/bacon-qr-code": "^2.0",
        "gellu/godaddy-api-client": "1.*",
        "ingria/laravel-x509-auth": "^1.1",
        "pragmarx/google2fa-laravel": "^1.0",
        "projectstage/godaddy-api-wrapper": "^1.0",

        "adldap2/adldap2-laravel": "^6.0",
        "fideloper/proxy": "^4.0",
        "forxer/laravel-gravatar": "^1.3",
        "geoip/geoip": "~1.14",
        "graham-campbell/digitalocean": "^5.3",
        "guzzlehttp/guzzle": "^6.4",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "kriswallsmith/buzz": "^0.16",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^1.0",
        "pragmarx/firewall": "^2.3",
        "ramsey/uuid": "^3.8",
        "spatie/laravel-activitylog": "^3.9",
        "spatie/laravel-permission": "^3.2",
        "toin0u/digitalocean-v2": "^2.2"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
